#define FONT_X  0
#define FONT_Y  476
#define FONT_HEIGHT 19
#define FONT_WIDTH  11

normalFont: normal

fonts: {

    normal:
    ( BitmapFont
        fontPosition: [ FONT_X  FONT_Y ]
        bitmapWidth: FONT_WIDTH
        bitmapHeight: FONT_HEIGHT
        characterSpacing: FONT_WIDTH
        lineSpacing: FONT_HEIGHT
        path: MIXR_DATA_FONT_11_19_PATH
        reverse: false
    )
}

#undef FONT_X
#undef FONT_Y
#undef FONT_HEIGHT
#undef FONT_WIDTH
